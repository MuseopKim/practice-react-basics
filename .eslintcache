s.number.isRequired,\n  imageUrl: PropTypes.string,\n  title: PropTypes.string.isRequired,\n  rating: PropTypes.number,\n  year: PropTypes.string,\n  isMovie: PropTypes.bool\n};\n\nexport default Poster;\n",{"ruleId":"107","replacedBy":"108"},{"ruleId":"109","replacedBy":"110"},{"ruleId":"111","severity":1,"message":"112","line":14,"column":1,"nodeType":"113","endLine":26,"endColumn":3},{"ruleId":"111","severity":1,"message":"114","line":5,"column":1,"nodeType":"113","endLine":54,"endColumn":2},{"ruleId":"111","severity":1,"message":"114","line":5,"column":1,"nodeType":"113","endLine":48,"endColumn":2},{"ruleId":"111","severity":1,"message":"114","line":5,"column":1,"nodeType":"113","endLine":64,"endColumn":2},{"ruleId":"111","severity":1,"message":"114","line":5,"column":1,"nodeType":"113","endLine":55,"endColumn":2},{"ruleId":"111","severity":1,"message":"112","line":13,"column":1,"nodeType":"113","endLine":13,"endColumn":47},{"ruleId":"115","severity":1,"message":"116","line":57,"column":17,"nodeType":"117","endLine":57,"endColumn":29},"no-native-reassign",["118"],"no-negated-in-lhs",["119"],"import/no-anonymous-default-export","Assign arrow function to a variable before exporting as module default","ExportDefaultDeclaration","Unexpected default export of anonymous class","jsx-a11y/aria-role","Elements with ARIA roles must use a valid, non-abstract ARIA role.","JSXAttribute","no-global-assign","no-unsafe-negation"]